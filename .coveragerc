[run]
source = kuranet
omit = 
    */tests/*
    */migrations/*
    */__init__.py
    */admin.py
    */apps.py
    */urls.py
    */conftest.py
    */polls/management/commands/seed.py
    */polls/permissions.py
    */kuranet/asgi.py
    */kuranet/wsgi.py
    */manage.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass